{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "urn:rdpms:core:schema:visualization-manifest:v1",
  "title": "VisualizationManifest",
  "type": "object",
  "description": "Visualization manifest for dataset content. The dataset linkage is managed by RDPMS.",
  "properties": {
    "id": {
      "type": "string",
      "format": "uuid",
      "description": "Manifest id."
    },
    "title": {
      "type": "string",
      "description": "Display title for the manifest."
    },
    "views": {
      "type": "array",
      "items": {
        "$ref": "visualization-view.v1.schema.json"
      }
    }
  },
  "required": [
    "id",
    "views"
  ],
  "additionalProperties": false
}
